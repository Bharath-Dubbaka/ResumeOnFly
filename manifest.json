{
   "manifest_version": 3,
   "name": "ResumeOnFly",
   "version": "1.0.3",
   "description": "Job Description Analyzer",
   "permissions": ["contextMenus", "storage", "identity"],
   "action": {
      "default_popup": "index.html"
   },
   "background": {
      "service_worker": "background.js",
      "type": "module"
   },
   "oauth2": {
      "client_id": "337280252650-h4dosfkg2ardr8e1npols2tm879kl1p7.apps.googleusercontent.com",
      "scopes": [
         "https://www.googleapis.com/auth/userinfo.email",
         "https://www.googleapis.com/auth/userinfo.profile"
      ]
   },
   "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
   }
}
